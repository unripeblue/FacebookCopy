<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false">
        <layer-list>
            <item>
                <shape>
                    <solid
                        android:color="#F5F5F5"/>

                </shape>

            </item>
            <item
                android:top="3dp"
                android:bottom="3dp"
                android:left="3dp"
                android:right="3dp">
                <shape
                    android:color="@color/white"/>
            </item>


        </layer-list>

    </item>

</selector>